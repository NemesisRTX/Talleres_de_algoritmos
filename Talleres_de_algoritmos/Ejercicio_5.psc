Algoritmo Inicio_calculadora
	Escribir "Inserte número 1"
	Leer numero_uno
	Escribir "Inserte número 2"
	Leer numero_dos
	Suma<-numero_uno+numero_dos
	Resta<-numero_uno-numero_dos
	Multiplicación<-numero_uno*numero_dos
	Division<-numero_uno/numero_dos
	Escribir "Resultado de suma = " Suma
	Escribir "Resultado de resta = " Resta
	Escribir "Resultado de multiplicación = " Multiplicación
	Escribir "Resultado de division = " Division
FinAlgoritmo
